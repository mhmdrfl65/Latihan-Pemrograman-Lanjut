public class MobilBeraksi {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Mobil MobilRafael = new Mobil();
        
        MobilRafael.warna = "Pink";
        MobilRafael.tahunProduksi = 1960;
                
       System.out.println("Warna: " + MobilRafael.warna);
       System.out.println("Tahun Produksi: " + MobilRafael.tahunProduksi);
    }
}
